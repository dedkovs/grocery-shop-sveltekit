{
	"name": "042-grocery-svelte",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme"
	},
	"devDependencies": {
		"@mdi/js": "^6.5.95",
		"@smui/button": "^6.0.0-beta.13",
		"@smui/card": "^6.0.0-beta.13",
		"@smui/icon-button": "^6.0.0-beta.13",
		"@smui/select": "^6.0.0-beta.13",
		"@smui/textfield": "^6.0.0-beta.13",
		"@smui/tooltip": "^6.0.0-beta.13",
		"@smui/top-app-bar": "^6.0.0-beta.13",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.67",
		"@sveltejs/kit": "next",
		"node-sass": "^7.0.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"smui-theme": "^6.0.0-beta.13",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"dotenv": "^15.0.0",
		"mongoose": "^6.1.8"
	}
}
